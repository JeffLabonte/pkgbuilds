# PKGBUILD for the paper maia icon theme

# Maintainer: Tony <tony@experimentwithlinux.proboards.com>
pkgname=paper-maia-icon-theme-git
pkgver=746.04115106
pkgrel=1
pkgdesc="Manjaro variation of Paper Icon Theme"
arch=('any')
url="https://experimentwithlinux.proboards.com"
license=('CC BY-SA 4.0')
groups=('mewle')
depends=('paper-icon-theme-git')
install=$pkgname.install
_commit=407498b18503
_initialdir=Tony-linux-$pkgname-$_commit
source=("$pkgname-$_commit.tar.gz::https://bitbucket.org/Tony-linux/paper-maia-icon-theme-git/get/$_commit.tar.gz")
md5sums=('SKIP')

package() {
cd "${srcdir}/"
install -d "${pkgdir}/usr/share/icons"
cp -r "${srcdir}/$_initialdir/Paper-Maia" "${pkgdir}/usr/share/icons/"
}
